<h1>Contact Us</h1>
<h1>Leave Comments</h1>
  <%= simple_form_for @contact do |f| %>
    <%= f.input :name %> 
    <%= f.input :email %>    
    <%= f.input :message, :as => :text, :required => false, :input_html => {:rows => 5}%>
    <div>
      </br>
      <%= f.button :submit, 'Send message', :class=> "btn btn-primary" %>
    </div>
  <% end %>

<h1>Contact Details</h1>
